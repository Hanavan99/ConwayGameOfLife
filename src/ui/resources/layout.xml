<?xml version="1.0" encoding="utf-8"?>
<layouts version="1.0">
	<extensions>
		<extension class="com.github.hanavan99.conwaygameoflife.ui.model.LayoutTransform" name="layout" />
	</extensions>
	<theme name="default">
		<panel name="MainPanel">
			<component name="main_title">
				<font arg0="Arial" arg1="PLAIN" arg2="72" />
				<layout>
					<position x="0" y="0" />
					<size width="100%" height="100" />
				</layout>
			</component>
			<component name="main_button1">
				<layout>
					<position x="50%" y="20%" />
					<margin x="-100" />
					<size width="200" height="30" />
				</layout>
			</component>
			<component name="main_button2">
				<layout>
					<position x="50%" y="20%" />
					<margin x="-100" y="40" />
					<size width="200" height="30" />
				</layout>
			</component>
			<component name="main_button3">
				<layout>
					<position x="50%" y="20%" />
					<margin x="-100" y="80" />
					<size width="200" height="30" />
				</layout>
			</component>
			<component name="main_button4">
				<layout>
					<position x="50%" y="20%" />
					<margin x="-100" y="120" />
					<size width="200" height="30" />
				</layout>
			</component>
			<component name="main_gif">
				<layout>
					<position x="50%" y="20%" />
					<margin x="-100" y="160" />
					<size width="200" height="200" />
				</layout>
			</component>
			<component name="navigation_panel">
				<layout>
					<position x="80%" y="80%" />
					<margin x="-100" y="120" />
					<size width="200" height="30" />
				</layout>
			</component>
			<component name="navigation_mainmenu">
				<layout>
					<position x="50%" />
					<margin x="-50" />
					<size width="100" height="30" />
				</layout>
			</component>
		</panel>
	</theme>
</layouts>
